; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "BreakBlock",
"language": "GDScript",
"path": "res://Common/BreakBlock/Breakblock.gd"
}, {
"base": "Area2D",
"class": "Ladder",
"language": "GDScript",
"path": "res://Ladder/Ladder.gd"
}, {
"base": "Node2D",
"class": "Level",
"language": "GDScript",
"path": "res://Levels/Level.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://Player/Player.gd"
}, {
"base": "Resource",
"class": "PlayerMovementData",
"language": "GDScript",
"path": "res://Player/PlayerMovementData.gd"
}, {
"base": "Area2D",
"class": "PowerUp",
"language": "GDScript",
"path": "res://PowerUps/BreakPowerUp.gd"
} ]
_global_script_class_icons={
"BreakBlock": "",
"Ladder": "",
"Level": "",
"Player": "",
"PlayerMovementData": "",
"PowerUp": ""
}

[application]

config/name="Platformer-tutorial"
run/main_scene="res://Levels/Level1.tscn"
config/icon="res://icon.png"

[audio]

default_bus_layout="res://Player/default_bus_layout.tres"

[autoload]

GameState="*res://Common/GameState.gd"
SoundPlayer="*res://Player/SoundPlayer.tscn"
Events="*res://Events/Events.gd"
Transitions="*res://Common/Transitions/Transitions.tscn"

[debug]

gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"

[global]

phy=false
main=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[layer_names]

2d_physics/layer_1="World"
2d_physics/layer_2="Characters"

[mono]

project/assembly_name="Platformer-tutorial"

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
